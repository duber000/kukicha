import "fmt"

type Config
    Name string json:"name"
    Version string json:"version"
    Debug bool json:"debug"

func main()
    # Struct tags are now supported!
    config := Config{Name: "myapp", Version: "1.0.0", Debug: true}
    
    fmt.Println("Struct with tags created successfully")
    fmt.Println(config.Name)
    fmt.Println("JSON tags work!")
